<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zl.frame.sysman.logman.bean">
    <class name="com.zl.frame.sysman.logman.bean.AccessLog" table="t_sys_access_log" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="userId" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="userName" />
        </property>
        <property name="loginTime" type="java.sql.Timestamp">
            <column name="loginTime" length="0" />
        </property>
        <property name="logoutTime" type="java.sql.Timestamp">
            <column name="logoutTime" length="0" />
        </property>
        <property name="logoutType" type="java.lang.Integer">
            <column name="logoutType" />
        </property>
        <property name="loginIp" type="java.lang.String">
            <column name="loginIp" />
        </property>
        <property name="sessionId" type="java.lang.String">
            <column name="sessionId" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" />
        </property>
    </class>
</hibernate-mapping>
