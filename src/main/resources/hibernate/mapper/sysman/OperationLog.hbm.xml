<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.zl.frame.sysman.logman.bean">
    <class name="com.zl.frame.sysman.logman.bean.OperationLog" table="t_sys_operation_log" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="userId" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="userName" />
        </property>
        <property name="operationTime" type="java.sql.Timestamp">
            <column name="operationTime" length="0" />
        </property>
        <property name="operationType" type="java.lang.Integer">
            <column name="operationType" />
        </property>
        <property name="operationUrl" type="java.lang.String">
            <column name="operationUrl" />
        </property>
        <property name="operationContent" type="java.lang.String">
            <column name="operationContent" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" />
        </property>
    </class>
</hibernate-mapping>
